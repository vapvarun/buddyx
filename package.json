  "type": "module",
  "scripts": {
    "setup": "npm install && composer install && node scripts/cli.js init",
    "dev": "node scripts/cli.js dev",
    "dev:js": "node scripts/build-js.js --dev",
    "dev:css": "node scripts/build-css.js --dev",
    "build": "node scripts/cli.js build",
    "build:js": "node scripts/build-js.js",
    "build:css": "node scripts/build-css.js",
    "bundle": "cross-env NODE_ENV=production node scripts/cli.js bundle",
    "dist": "node scripts/distribute.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "cross-env ESLINT_USE_FLAT_CONFIG=true eslint \"assets/js/src/**/*.{js,jsx,ts,tsx}\"",
    "lint:css": "npx stylelint \"assets/css/src/**/*.css\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:perf": "lhci autorun",
    "test:perf:collect": "lhci collect",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "build:blocks": "node scripts/build-all-blocks.js",
    "start:blocks": "node scripts/build-all-blocks.js --watch"
  },
  "dependencies": {
    "draggabilly": "^3.0.0",
    "glob": "^11.0.3",
    "unidragger": "^3.0.1"
  }
